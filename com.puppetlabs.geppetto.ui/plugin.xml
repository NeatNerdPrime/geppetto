<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension point="org.eclipse.ui.newWizards">
      <category
            id="com.puppetlabs.geppetto.ui.newWizardCategory"
            name="%_UI_Wizard_Category"/>
      <wizard
            category="com.puppetlabs.geppetto.ui.newWizardCategory"
            class="com.puppetlabs.geppetto.ui.GeppettoExecutableExtensionFactory:com.puppetlabs.geppetto.ui.wizard.NewPuppetProjectWizard"
            icon="icons/full/etool16/NewPuppetProject.gif"
            id="com.puppetlabs.geppetto.ui.NewPuppetProjectWizard"
            name="%_UI_PuppetProject_title"
            project="true">
         <description>
            %_UI_PuppetProject_description
         </description>
         <selection
               class="org.eclipse.core.resources.IResource">
         </selection>
      </wizard>
      <wizard
            id="com.puppetlabs.geppetto.ui.NewPuppetModuleWizard"
            name="%_UI_PuppetModule_title"
            class="com.puppetlabs.geppetto.ui.GeppettoExecutableExtensionFactory:com.puppetlabs.geppetto.ui.wizard.NewPuppetModuleProjectWizard"
            category="com.puppetlabs.geppetto.ui.newWizardCategory"
            project="true"
            icon="icons/full/etool16/NewPuppetProject.gif">
         <description>%_UI_PuppetModule_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
      <wizard
            category="com.puppetlabs.geppetto.ui.newWizardCategory"
            class="com.puppetlabs.geppetto.ui.wizard.NewManifestWizard"
            icon="icons/obj16/new_pp.png"
            id="com.puppetlabs.geppetto.ui.NewManifestWizard"
            name="New Puppet Manifest">
         <selection
               class="org.eclipse.core.resources.IResource">
         </selection>
         <description>
            %_UI_NewPuppetManifest_description
         </description>
      </wizard>
      <wizard
            category="com.puppetlabs.geppetto.ui.newWizardCategory"
            class="com.puppetlabs.geppetto.ui.wizard.NewMetadataFileWizard"
            icon="icons/obj16/puppet-16.png"
            id="com.puppetlabs.geppetto.ui.NewMetadataFileWizard"
            name="New Puppet Metadata">
         <selection
               class="org.eclipse.core.resources.IResource">
         </selection>
         <description>
            %_UI_NewPuppetMetadata_description
         </description>
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.exportWizards">
      <category
            id="com.puppetlabs.geppetto.ui.exportWizardCategory"
            name="%_UI_Wizard_Category">
      </category>
      <wizard
            category="com.puppetlabs.geppetto.ui.exportWizardCategory"
            class="com.puppetlabs.geppetto.ui.GeppettoExecutableExtensionFactory:com.puppetlabs.geppetto.ui.wizard.ModuleExportToForgeWizard"
            icon="icons/obj16/puppet-16.png"
            id="com.puppetlabs.geppetto.ui.ModuleExportToForgeWizard"
            name="%_UI_ExportModulesToForge">
      </wizard>
      <wizard
            category="com.puppetlabs.geppetto.ui.exportWizardCategory"
            class="com.puppetlabs.geppetto.ui.GeppettoExecutableExtensionFactory:com.puppetlabs.geppetto.ui.wizard.ModuleExportToFileWizard"
            icon="icons/obj16/puppet-16.png"
            id="com.puppetlabs.geppetto.ui.ModuleExportToFileWizard"
            name="%_UI_ExportModulesToFileSystem">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.importWizards">
      <category
            id="com.puppetlabs.geppetto.ui.importWizardCategory"
            name="%_UI_Wizard_Category">
      </category>
      <wizard
            category="com.puppetlabs.geppetto.ui.importWizardCategory"
            class="com.puppetlabs.geppetto.ui.GeppettoExecutableExtensionFactory:com.puppetlabs.geppetto.ui.wizard.ImportPuppetModuleFromSourceWizard"
            icon="icons/full/etool16/NewPuppetProject.gif"
            id="com.puppetlabs.geppetto.ui.ImportPuppetModuleFromSourceWizard"
            name="%_UI_PuppetModuleFromSource_title">
         <description>
            %_UI_PuppetModuleFromSource_description
         </description>
      </wizard>
      <wizard
            category="com.puppetlabs.geppetto.ui.importWizardCategory"
            class="com.puppetlabs.geppetto.ui.GeppettoExecutableExtensionFactory:com.puppetlabs.geppetto.ui.wizard.ImportPuppetModuleFromForgeWizard"
            icon="icons/full/etool16/NewPuppetProject.gif"
            id="com.puppetlabs.geppetto.ui.ImportPuppetModuleFromForgeWizard"
            name="%_UI_PuppetModuleFromForge_title">
         <description>
            %_UI_PuppetModuleFromForge_description
         </description>
      </wizard>
   </extension>
</plugin>
