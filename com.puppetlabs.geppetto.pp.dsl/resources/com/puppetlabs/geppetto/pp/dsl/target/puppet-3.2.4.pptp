    <properties name="keepalive"/>
    <properties name="enabled"/>
    <properties name="proxy_password"/>
    <properties name="sslclientkey"/>
    <properties name="exclude"/>
    <properties name="http_caching"/>
    <properties name="cost"/>
    <properties name="include"/>
