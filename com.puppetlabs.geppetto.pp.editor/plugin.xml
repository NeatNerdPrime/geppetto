<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated PPTP -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="com.puppetlabs.geppetto.pp.pptp.presentation.PPTPModelWizardID"
            name="%_UI_PPTPModelWizard_label"
            class="com.puppetlabs.geppetto.pp.pptp.presentation.PPTPModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/PPTPModelFile.gif">
         <description>%_UI_PPTPModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated PPTP -->
      <editor
            id="com.puppetlabs.geppetto.pp.pptp.presentation.PPTPEditorID"
            name="%_UI_PPTPEditor_label"
            icon="icons/full/obj16/PPTPModelFile.gif"
            extensions="pptp"
            class="com.puppetlabs.geppetto.pp.pptp.presentation.PPTPEditor"
            contributorClass="com.puppetlabs.geppetto.pp.pptp.presentation.PPTPActionBarContributor">
      </editor>
   </extension>

</plugin>
